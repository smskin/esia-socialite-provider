{
    "name": "smskin/esia-socialite-provider",
    "description": "ESIA Provider for Laravel Socialite",
    "license": "MIT",
    "keywords": [
        "laravel",
        "esia",
        "oauth",
        "provider",
        "socialite"
    ],
    "authors": [
        {
            "name": "Sergey Mikhaylov",
            "email": "sms7@yandex.ru"
        }
    ],
    "homepage": "https://github.com/smskin/esia-socialite-provider",
    "support": {
        "issues": "https://github.com/smskin/esia-socialite-provider/issues",
        "source": "https://github.com/smskin/esia-socialite-provider"
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "socialiteproviders/manager": "~4.0"
    },
    "suggest": {
        "ext-openssl": "SignerPKCS7 support"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SMSkin\\SocialiteProviders\\ESIA\\Providers\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "SMSkin\\SocialiteProviders\\ESIA\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
